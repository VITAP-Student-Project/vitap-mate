{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    "store:default",
    "notification:default",
    "opener:default",
    "nativeapi:default",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "/storage/emulated/0/Download/*"
        }
      ]
    }
  ]
}